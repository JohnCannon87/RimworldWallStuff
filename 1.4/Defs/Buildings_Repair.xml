<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="WallBuildingBase">
        <defName>Nano_Cloud_Dispenser</defName>
        <label>nano cloud dispenser</label>
        <description>A small wall mounted nano cloud dispenser.</description>
        <graphicData>
            <texPath>WallRepair/WallRepair</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
        </graphicData>
        <thingCategories>
            <li>BuildingsSpecial</li>
        </thingCategories>
        <uiIconPath>WallRepair/WallRepair_UI</uiIconPath>
        <costList>
            <Gold>250</Gold>
            <Plasteel>200</Plasteel>
            <ComponentSpacer>15</ComponentSpacer>
        </costList>
        <building>
            <isEdifice>false</isEdifice>
        </building>
        <statBases>
            <WorkToBuild>3000</WorkToBuild>
            <MaxHitPoints>1000</MaxHitPoints>
            <Flammability>1.0</Flammability>
            <Mass>35</Mass>
            <MarketValue>3000</MarketValue>
        </statBases>
        <specialDisplayRadius>5.9</specialDisplayRadius>
        <tickerType>Rare</tickerType>
        <comps>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <basePowerConsumption>10</basePowerConsumption>
            </li>
            <li Class="CompProperties_Breakdownable"/>
            <li Class="CompProperties_Flickable"/>
            <li Class="WallStuff.WallMountedCompProperties_Repair">
                <requiresPower>true</requiresPower>
                <NormalPowerUsage>1</NormalPowerUsage>
                <OverpowerUsage>10</OverpowerUsage>
            </li>
        </comps>
        <placeWorkers>
            <li>WallStuff.PlaceWorker_WallObject</li>
            <li>WallStuff.PlaceWorker_WallChecker</li>
        </placeWorkers>
        <designationCategory>Production</designationCategory>
        <clearBuildingArea>false</clearBuildingArea>
        <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
        <altitudeLayer>PawnUnused</altitudeLayer>
        <castEdgeShadows>false</castEdgeShadows>
        <researchPrerequisites>
            <li>MatterToEnergyConversion</li>
        </researchPrerequisites>
        <minifiedDef>MinifiedThing</minifiedDef>
        <passability>Impassable</passability>
    </ThingDef>

</Defs>
